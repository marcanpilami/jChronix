{
    "editor.formatOnSave": true,
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.format.settings.url": "./chronix-doc/src/site/sphinx/code/java.xml",
    "java.import.exclusions": [
        "**/.*",
        "**/node_modules/**",
        "**/.metadata/**",
        "**/archetype-resources/**",
        "**/META-INF/maven/**",
        "**/target/",
        "**/webapp/dist/"
    ],
    "restructuredtext.confPath": "${workspaceFolder}\\chronix-doc\\src\\site\\sphinx",
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "restructuredtext"
    ],
}